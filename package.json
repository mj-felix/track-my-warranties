{
  "name": "track-my-warranties",
  "version": "0.9.9",
  "description": "App to track warranties on purchased items",
  "main": "app.js",
  "scripts": {
    "updev": "docker-compose -f docker-compose-dev.yml up -d",
    "build-dev": "docker-compose -f docker-compose-dev.yml up --build -d",
    "downdev": "docker-compose -f docker-compose-dev.yml down",
    "logs": "docker logs -f app",
    "dev": "nodemon app.js",
    "start": "node app.js",
    "notifications": "docker exec -it app node sendNotificationsSchedule.js",
    "format": "docker exec -it app npx prettier --write './**/*.js'",
    "check-ejs": "docker exec -it app npx prettier --check './**/*.ejs'"
  },
  "author": "MJ Felix",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "aws-sdk": "^2.883.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^4.4.1",
    "csurf": "^1.11.0",
    "dateformat": "^4.5.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "ejs-mate": "^3.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.2",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "joi": "^17.4.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.12.3",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nocache": "^3.0.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "s3-proxy": "^1.2.1",
    "sanitize-html": "^2.3.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "morgan": "^1.10.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.6.2"
  }
}
